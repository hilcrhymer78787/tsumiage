{
    "extends": "next/core-web-vitals",
    "rules": {
        "prefer-const": "error",
        "no-var": "error",
        "quotes": "error",
        "space-before-function-paren": "error",
        "indent": [
            "error",
            4
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 0
            }
        ],
        "semi": "error",
        "react-hooks/exhaustive-deps": "off",
        "react/jsx-boolean-value": "error",
        "react/jsx-curly-brace-presence": "error"
    }
}